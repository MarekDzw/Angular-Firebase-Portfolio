{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetc",
      "cacheConfig": {
        "maxAge": "7d12h ",
        "strategy": "performance"
      },
      "resources": {
        "files": ["/assets/**"],
        "urls": [
          "https://fonts.googleapis.com/css?family=Oswald:300,400,500|Roboto:100,400,400i",
          "https://angular-firebase-api.firebaseio.com/**"
        ]
      }
    }
  ]
}
